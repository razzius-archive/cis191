#!/bin/python

import pickle, sys

try:
    agents = pickle.load(open("agents.p", "rb"))
except IOError:
    agents = {}
agent = sys.argv[1]
if agent in agents:
    agents[agent] = agents[agent] + 1
else:
    agents[agent] = 1
for k in sorted(agents):
    print("{} with freq {}".format(k,agents[k]))

pickle.dump(agents, open("agents.p", "wb"))
